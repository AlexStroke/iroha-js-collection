{
  "name": "@iroha2/client",
  "version": "2.0.0",
  "module": "dist/lib.mjs",
  "main": "dist/lib.cjs",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.mjs",
      "require": "./dist/lib.cjs",
      "types": "./dist/lib.d.ts"
    },
    "./web-socket/native": {
      "import": "./dist/web-socket/native.mjs",
      "require": "./dist/web-socket/native.cjs",
      "types": "./dist/web-socket/native.d.ts"
    },
    "./web-socket/node": {
      "import": "./dist/web-socket/node.mjs",
      "require": "./dist/web-socket/node.cjs",
      "types": "./dist/web-socket/node.d.ts"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "license": "Apache 2.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@iroha2/crypto-core": "^0.1.1",
    "@iroha2/data-model": "2.0.0",
    "@types/ws": "^8.2.2",
    "debug": "^4.3.4",
    "emittery": "^0.10.1",
    "json-bigint": "^1.0.0",
    "ws": "^8.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "consola": "^2.15.3",
    "npm-run-all": "^4.1.5",
    "type-fest": "^2.18.0"
  },
  "scripts": {
    "test:integration": "run-s test:prepare test:node test:web",
    "test:prepare": "tsx test/integration/prepare-peer",
    "test:node": "pnpm --filter client-test-node test",
    "test:web": "pnpm --filter client-test-web test",
    "build:tsc": "tsc"
  }
}